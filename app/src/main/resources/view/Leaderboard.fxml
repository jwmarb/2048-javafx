<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.*?>
<?import javafx.scene.control.*?>
<?import org.csc335.controllers.Pressable?>

<fx:root type="javafx.scene.layout.BorderPane" xmlns:fx="http://javafx.com/fxml/1">
  <left>
    <Pressable text="Back" onAction="#goBack" variant="filled" />
  </left>
  <center>
    <VBox styleClass="leaderboard-container">
      <VBox styleClass="title-container">
        <Label styleClass="leaderboard-title-label" text="Top five scores!" />
      </VBox>
      <VBox styleClass="scores-container">
        <Label styleClass="leaderboard-label" text="0" fx:id="top5" />
      </VBox>
    </VBox>
  </center>
</fx:root>